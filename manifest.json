{
  "applications": {
    "gecko": {
      "id": "thunderbird-tabs@joschka-huellmann.net",
      "strict_min_version": "68.6.0",
      "strict_max_version": "95.*"
    }
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "browser_style": true,
    "default_title": "Open Google Tabs",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "64": "icons/64.png",
      "128": "icons/128.png"
    }
  },
  "developer": {
    "name": "Joschka HÃ¼llmann",
    "url": "https://www.joschka-huellmann.et"
  },  
  "description": "Open up Google Calendar, Notes, and Microsoft Outlook in a new tab",
  "manifest_version": 2,
  "name": "Open Google Tabs",
  "version": "1.2",
  "permissions": [
    "tabs",
    "webNavigation",
    "cookies"
  ]
}
