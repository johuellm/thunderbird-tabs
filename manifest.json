{
  "manifest_version": 3,
  "name": "Open Google Tabs",
  "description": "Open up Google Calendar, Notes, and Microsoft Outlook in a new tab",
  "version": "1.3",
  "developer": {
    "name": "Joschka HÃ¼llmann",
    "url": "https://github.com/johuellm/thunderbird-tabs"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "thunderbird-tabs@joschka-huellmann.net",
      "strict_min_version": "109.0"
    }
  },
  "action": {
    "default_title": "Open Google Tabs",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "64": "icons/64.png",
      "128": "icons/128.png"
    }
  },
   "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "64": "icons/64.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "tabs",
    "webNavigation",
    "cookies"
  ]
}
